{
  "manifest_version": 2,
  "name": "dns-spy Subdomain Collector",
  "description": "This extension colllects subdomain information for the dns-spy project.",
  "version": "1.0",

  "permissions": [
	"webRequest",
	"<all_urls>"    
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["collector.js"],
    "persistent": true
  }
}
